        "foreground": "#f07178",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#f07178",
        "fontStyle": "bold"
      }
    },
